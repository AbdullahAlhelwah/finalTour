<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.finaltour.teamPlayerController">
   <children>
      <TableView fx:id="table" layoutX="157.0" layoutY="100.0" prefHeight="328.0" prefWidth="302.0" stylesheets="@table_style.css">
        <columns>
          <TableColumn fx:id="name" prefWidth="150.0" text="Player Name" />
          <TableColumn fx:id="id" prefWidth="150.0" text="Player ID" />
        </columns>
      </TableView>
      <Button layoutX="317.0" layoutY="481.0" mnemonicParsing="false" onAction="#toTeamList" prefHeight="26.0" prefWidth="103.0" text="Go Back" />
      <Button layoutX="440.0" layoutY="481.0" mnemonicParsing="false" onAction="#toPlayerProfile" prefHeight="26.0" prefWidth="103.0" text="Player Profile" />
      <Label fx:id="teamName" alignment="CENTER" layoutX="157.0" layoutY="46.0" prefHeight="18.0" prefWidth="302.0" text="Team Name" textFill="DARKBLUE">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
